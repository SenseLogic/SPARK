// -- FUNCTIONS

function GetValue(
    value,
    default_value = ''
    )
{
    if ( isset( value ) )
    {
        return value;
    }
    else
    {
        return default_value;
    }
}

// ~~

function HasProperty(
    object &object_,
    string property_name
    )
{
    return property_exists( object_, property_name );
}
