<div>
    <? foreach ( var product; .ProductArray ) { ?>
        <div>
            <h2><% product.Name %></h2>
            <p><% product.Text %></p>
            <div>
                <img src="/upload/image/<# product.Image #>"/>
            </div>
            <div>
                <video src="/upload/video/<# product.Video #>" autoplay loop>
                </video>
            </div>
        </div>
    <? } ?>
</div>
