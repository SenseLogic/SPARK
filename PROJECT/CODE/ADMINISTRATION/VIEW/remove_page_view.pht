                    <% GetTextBySlug( 'Is Active' ) %> :
                </div>
                <div>
                    <select class="form-select" name="IsActive" readonly>
                        <option value="0"<? if ( .Page.IsActive === '0' ) echo ' selected'; ?>>False</option>
                        <option value="1"<? if ( .Page.IsActive === '1' ) echo ' selected'; ?>>True</option>
                    </select>
                </div>
                <div class="form-field-name">
                    <% GetTextBySlug( 'Title' ) %> :
                </div>
