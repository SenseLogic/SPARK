<? include 'BLOCK/page_header.php'; ?>
<div id="remove-page-view">
    <div class="page-section form-section">
        <form class="form-centered" action="/admin/page/remove/<% .Page.Id %>" method="post">
            <div class="form-container">
                <div class="form-field-name">
                    <% GetTextBySlug( 'Slug' ) %> :
                </div>
                <div>
                    <input-component result-class="form-input" result-name="Slug" type_="text" result-value="<% GetValueText( .Page.Slug ) %>" result-readonly></input-component>
                </div>
                <div class="form-field-name">
                    <% GetTextBySlug( 'Route' ) %> :
                </div>
                <div>
                    <input-component result-class="form-input" result-name="Route" type_="text" result-value="<% GetValueText( .Page.Route ) %>" result-readonly></input-component>
                </div>
                <div class="form-field-name">
                    <% GetTextBySlug( 'Type Slug' ) %> :
                </div>
                <div>
                    <select class="form-select" name="TypeSlug" readonly>
                        <? var page_type_array = .PageTypeArray; ?>
                        <? foreach ( var page_type; page_type_array ) { ?>
                            <option value="<% GetValueText( page_type.Slug ) %>"<? if ( .Page.TypeSlug === page_type.Slug ) echo ' selected'; ?>><% GetUntranslatedText( page_type.Title ) %></option>
                        <? } ?>
                    </select>
                </div>
                <div class="form-field-name">
                    <% GetTextBySlug( 'Number' ) %> :
                </div>
                <div>
                    <input-component result-class="form-input" result-name="Number" type_="text" result-value="<% GetValueText( .Page.Number ) %>" result-readonly></input-component>
                </div>
                <div class="form-field-name">
                    <% GetTextBySlug( 'Language Code Array' ) %> :
                </div>
